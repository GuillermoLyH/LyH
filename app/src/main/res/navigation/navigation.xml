<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.guillermolyh.lyh.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_aduanaFragment"
            app:destination="@id/aduanaFragment" />
        <action
            android:id="@+id/action_homeFragment_to_asesoriaFragment"
            app:destination="@id/asesoriaFragment" />
        <action
            android:id="@+id/action_homeFragment_to_capacitacionesFragment"
            app:destination="@id/capacitacionesFragment" />
        <action
            android:id="@+id/action_homeFragment_to_comprasFragment"
            app:destination="@id/comprasFragment" />
        <action
            android:id="@+id/action_homeFragment_to_consultoriaFragment"
            app:destination="@id/consultoriaFragment" />
        <action
            android:id="@+id/action_homeFragment_to_judicialFragment"
            app:destination="@id/judicialFragment" />
    </fragment>
    <fragment
        android:id="@+id/aduanaFragment"
        android:name="com.guillermolyh.lyh.AduanaFragment"
        android:label="fragment_aduana"
        tools:layout="@layout/fragment_aduana" />
    <fragment
        android:id="@+id/asesoriaFragment"
        android:name="com.guillermolyh.lyh.AsesoriaFragment"
        android:label="fragment_asesoria"
        tools:layout="@layout/fragment_asesoria" />
    <fragment
        android:id="@+id/capacitacionesFragment"
        android:name="com.guillermolyh.lyh.CapacitacionesFragment"
        android:label="fragment_capacitaciones"
        tools:layout="@layout/fragment_capacitaciones" />
    <fragment
        android:id="@+id/comprasFragment"
        android:name="com.guillermolyh.lyh.ComprasFragment"
        android:label="fragment_compras"
        tools:layout="@layout/fragment_compras" />
    <fragment
        android:id="@+id/consultoriaFragment"
        android:name="com.guillermolyh.lyh.ConsultoriaFragment"
        android:label="fragment_consultoria"
        tools:layout="@layout/fragment_consultoria" />
    <fragment
        android:id="@+id/judicialFragment"
        android:name="com.guillermolyh.lyh.JudicialFragment"
        android:label="fragment_judicial"
        tools:layout="@layout/fragment_judicial" />
    <fragment
        android:id="@+id/suscribirseFragment"
        android:name="com.guillermolyh.lyh.SuscribirseFragment"
        android:label="fragment_suscribirse"
        tools:layout="@layout/fragment_suscribirse" />
    <fragment
        android:id="@+id/contactFragment"
        android:name="com.guillermolyh.lyh.ContactFragment"
        android:label="fragment_contact"
        tools:layout="@layout/fragment_contact" />
    <fragment
        android:id="@+id/documentoFragment"
        android:name="com.guillermolyh.lyh.DocumentoFragment"
        android:label="fragment_documento"
        tools:layout="@layout/fragment_documento" />
    <fragment
        android:id="@+id/webinarFragment"
        android:name="com.guillermolyh.lyh.WebinarFragment"
        android:label="fragment_webinar"
        tools:layout="@layout/fragment_webinar" />
    <fragment
        android:id="@+id/evaluenosFragment"
        android:name="com.guillermolyh.lyh.EvaluenosFragment"
        android:label="fragment_evaluenos"
        tools:layout="@layout/fragment_evaluenos" />
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.guillermolyh.lyh.AboutFragment"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" >
        <action
            android:id="@+id/action_aboutFragment_to_historiaFragment"
            app:destination="@id/historiaFragment" />
        <action
            android:id="@+id/action_aboutFragment_to_clientesFragment"
            app:destination="@id/clientesFragment" />
        <action
            android:id="@+id/action_aboutFragment_to_usFragment"
            app:destination="@id/usFragment" />
    </fragment>
    <fragment
        android:id="@+id/historiaFragment"
        android:name="com.guillermolyh.lyh.HistoriaFragment"
        android:label="fragment_historia"
        tools:layout="@layout/fragment_historia" />
    <fragment
        android:id="@+id/clientesFragment"
        android:name="com.guillermolyh.lyh.ClientesFragment"
        android:label="fragment_clientes"
        tools:layout="@layout/fragment_clientes" />
    <fragment
        android:id="@+id/usFragment"
        android:name="com.guillermolyh.lyh.UsFragment"
        android:label="fragment_us"
        tools:layout="@layout/fragment_us" />
</navigation>